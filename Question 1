import java.io.FileNotFoundException;
import java.util.Scanner;
import java.io.File;

public class Lexical 
{
	public static void main(String[] args) throws FileNotFoundException
	{
		// uncomment to reveal Testing case
		//readSource("test_2");
	}
	
	public static void readSource(String name) throws FileNotFoundException
	{
		// Initialize the Scanner & File utility to read from the user
		Scanner input = new Scanner(new File(name));
		
		String line = "";
		
		while(input.hasNext()) 
		{
			// Save the first line in the line variable
			line = input.nextLine();
			
			
			//Array that will hold each word after removing white space
			String[] tokens = line.split(" ");
			
			// Looping through the array
			for(String token : tokens) 
			{
				// Check for String
				if(token.matches(".*[a-zA-Z]")) 
				{
					System.out.println(token + " => " + "String");
				}
				
				// Check for Integer
				else if(token.matches(".*[0-9]")) 
				{
					System.out.println(token + " => " + "Integer");
				}
				
				// Check for float
				else if(token.matches("[+-]?([0-9]*[.])?[0-9]+")) 
				{
					System.out.println(token + " => " + "Float");
				}
				// Check for a variable, 
				else if(token.matches("[a-zA-Z_][a-zA-Z0-9_]*")) 
				{
					System.out.println(token + " => " + "variable");
				}
			}
			
		}
		
	}
import java.io.FileNotFoundException;
import java.util.Scanner;
import java.io.File;

public class Lexical 
{
	public static void main(String[] args) throws FileNotFoundException
	{
		// uncomment to reveal Testing case
		//readSource("test_2");
	}
	
	public static void readSource(String name) throws FileNotFoundException
	{
		// Initialize the Scanner & File utility to read from the user
		Scanner input = new Scanner(new File(name));
		
		String line = "";
		
		while(input.hasNext()) 
		{
			// Save the first line in the line variable
			line = input.nextLine();
			
			
			//Array that will hold each word after removing white space
			String[] tokens = line.split(" ");
			
			// Looping through the array
			for(String token : tokens) 
			{
				// Check for String
				if(token.matches(".*[a-zA-Z]")) 
				{
					System.out.println(token + " => " + "String");
				}
				
				// Check for Integer
				else if(token.matches(".*[0-9]")) 
				{
					System.out.println(token + " => " + "Integer");
				}
				
				// Check for float
				else if(token.matches("[+-]?([0-9]*[.])?[0-9]+")) 
				{
					System.out.println(token + " => " + "Float");
				}
				// Check for a variable, 
				else if(token.matches("[a-zA-Z_][a-zA-Z0-9_]*")) 
				{
					System.out.println(token + " => " + "variable");
				}
			}
			
		}
		
	}
}
